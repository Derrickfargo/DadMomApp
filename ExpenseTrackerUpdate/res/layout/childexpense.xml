<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical" >

 <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="54.0dip"
        android:background="@drawable/header"
        android:weightSum="9.0" >

        <ImageView
            android:id="@+id/logOut"
            android:layout_width="wrap_content"
            android:layout_height="35.0dip"
            android:layout_marginTop="10.0dip"
            android:layout_weight="3.0"
            android:src="@drawable/logout" />

        <TextView
            android:id="@+id/Text"
            android:layout_width="wrap_content"
            android:layout_height="40.0dip"
            android:layout_weight="3.0"
            android:gravity="center_vertical"
            android:text="I\&apos;m between!"
            android:textColor="@color/black"
            android:textSize="20.0dip" />

        <ImageView
            android:id="@+id/editProfile"
            android:layout_width="wrap_content"
            android:layout_height="35.0dip"
            android:layout_marginTop="10.0dip"
            android:layout_weight="3.0"
            android:src="@drawable/edit_profile" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <ScrollView
            android:layout_width="fill_parent"
            android:layout_height="fill_parent" >

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:background="@drawable/home_bakground"
                android:weightSum="12.0" >

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_margin="5.0dip"
                    android:layout_weight="10.0"
                    android:background="@drawable/shapes"
                    android:orientation="vertical" >

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="4.0dip"
                        android:layout_marginTop="4.0dip"
                        android:layout_weight="1.0"
                        android:orientation="horizontal" >

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="fill_parent"
                            android:layout_gravity="center"
                            android:layout_marginLeft="3.0dip"
                            android:layout_weight="0.3"
                            android:orientation="horizontal" >

                            <TextView
                                android:id="@+id/signup_textView_username"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:text="Date:"
                                android:textColor="#ffffffff"
                                android:textSize="15.0sp" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="fill_parent"
                            android:layout_marginRight="3.0dip"
                            android:background="@drawable/background_textbox"
                            android:orientation="horizontal" >

                            <TextView
                                android:id="@+id/expenseDateText"
                                android:layout_width="160.0dip"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="12.0dip"
                                android:layout_marginTop="15.0dip"
                                android:text=""
                                android:textColor="#ffffffff"
                                android:textSize="15.0sp" />

                            <ImageView
                                android:id="@+id/expenseDateImage"
                                android:layout_width="50.0dip"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="8.0dip"
                                android:src="@drawable/rightarrow" />
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="4.0dip"
                        android:layout_marginTop="4.0dip"
                        android:layout_weight="1.0"
                        android:orientation="horizontal" >

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="fill_parent"
                            android:layout_gravity="center"
                            android:layout_marginLeft="3.0dip"
                            android:layout_weight="0.3"
                            android:orientation="horizontal" >

                            <TextView
                                android:id="@+id/dollar"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:text="$:"
                                android:textColor="#ffffffff"
                                android:textSize="15.0sp" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="fill_parent"
                            android:layout_marginRight="3.0dip"
                            android:orientation="horizontal" >

                            <EditText
                                android:id="@+id/dollar_money"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:background="@drawable/background_textbox"
                                android:ems="10"
                                android:inputType="none"
                                android:paddingLeft="15.0dip"
                                android:paddingRight="15.0dip"
                                android:singleLine="true" >

                                <requestFocus />
                            </EditText>
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="4.0dip"
                        android:layout_marginTop="4.0dip"
                        android:layout_weight="1.0"
                        android:orientation="horizontal" >

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="fill_parent"
                            android:layout_gravity="center"
                            android:layout_marginLeft="3.0dip"
                            android:layout_weight="0.3"
                            android:orientation="horizontal" >

                            <TextView
                                android:id="@+id/category_child"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:text="Category:"
                                android:textColor="#ffffffff"
                                android:textSize="15.0sp" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="fill_parent"
                            android:layout_marginRight="3.0dip"
                            android:orientation="horizontal" >

                            <Spinner
                                android:id="@+id/SpinnerCategory"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content" />
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="4.0dip"
                        android:layout_marginTop="5.0dip"
                        android:layout_weight="1.0"
                        android:orientation="horizontal" >

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="fill_parent"
                            android:layout_gravity="center"
                            android:layout_marginLeft="3.0dip"
                            android:layout_weight="0.3"
                            android:orientation="horizontal" >

                            <TextView
                                android:id="@+id/signup_textView_name"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:text="Payment:"
                                android:textColor="#ffffffff"
                                android:textSize="15.0sp" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="fill_parent"
                            android:layout_marginRight="3.0dip"
                            android:orientation="horizontal" >

                            <Spinner
                                android:id="@+id/spinnerPayment"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content" />
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="4.0dip"
                        android:layout_marginTop="4.0dip"
                        android:layout_weight="1.0"
                        android:orientation="horizontal" 
                        >

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="fill_parent"
                            android:layout_gravity="center"
                            android:layout_marginLeft="3.0dip"
                            android:layout_weight="0.3"
                            android:orientation="horizontal" >

                            <TextView
                                android:id="@+id/signup_textView_username"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:text="Description"
                                android:textColor="#ffffffff"
                                android:textSize="15.0sp" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="fill_parent"
                            android:layout_marginRight="3.0dip"
                            android:background="@drawable/background_textbox"
                            android:orientation="horizontal" >

                            <TextView
                                android:id="@+id/expenseDescriptionText"
                                android:layout_width="160.0dip"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="12.0dip"
                                android:layout_marginTop="15.0dip"
                                android:paddingLeft="15.0dip"
                                android:paddingRight="15.0dip"
                                android:singleLine="true"
                                android:text=""
                                android:maxLines="1"
                                android:textColor="#ffffffff"
                                android:textSize="15.0sp" />

                            <ImageView
                                android:id="@+id/expenseDescriptionImage"
                                android:layout_width="50.0dip"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="8.0dip"
                                android:src="@drawable/rightarrow" />
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="4.0dip"
                        android:layout_marginTop="4.0dip"
                        android:layout_weight="1.0"
                        android:orientation="horizontal" >

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="fill_parent"
                            android:layout_gravity="center"
                            android:layout_marginLeft="3.0dip"
                            android:layout_weight="0.3"
                            android:orientation="horizontal" >

                            <TextView
                                android:id="@+id/signup_textView_email"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:text="Location:"
                                android:textColor="#ffffffff"
                                android:textSize="15.0sp" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="fill_parent"
                            android:layout_marginRight="3.0dip"
                            android:background="@drawable/background_textbox"
                            android:orientation="horizontal" >

                            <TextView
                                android:id="@+id/expenseLocationText"
                                android:layout_width="160.0dip"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="12.0dip"
                                android:layout_marginTop="15.0dip"
                                android:paddingLeft="15.0dip"
                                android:paddingRight="15.0dip"
                                android:singleLine="true"
                                android:text=""
                                android:textColor="#ffffffff"
                                android:textSize="15.0sp" />

                            <ImageView
                                android:id="@+id/expenseLocation"
                                android:layout_width="50.0dip"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="8.0dip"
                                android:src="@drawable/location" />
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="4.0dip"
                        android:layout_marginTop="4.0dip"
                        android:layout_weight="1.0"
                        android:orientation="horizontal" >

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="fill_parent"
                            android:layout_gravity="center"
                            android:layout_marginLeft="3.0dip"
                            android:layout_weight="0.3"
                            android:orientation="horizontal" >

                            <TextView
                                android:id="@+id/signup_textView_phone"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:text="Audio:"
                                android:textColor="#ffffffff"
                                android:textSize="15.0sp" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="fill_parent"
                            android:layout_marginRight="3.0dip"
                            android:background="@drawable/background_textbox"
                            android:orientation="horizontal" >

                            <TextView
                                android:id="@+id/audioText"
                                android:layout_width="160.0dip"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="12.0dip"
                                android:layout_marginTop="15.0dip"
                                android:paddingLeft="15.0dip"
                                android:paddingRight="15.0dip"
                                android:singleLine="true"
                                android:text=""
                                android:textColor="#ffffffff"
                                android:textSize="15.0sp" />

                            <ImageView
                                android:id="@+id/audio"
                                android:layout_width="50.0dip"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="8.0dip"
                                android:src="@drawable/audio" />
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="4.0dip"
                        android:layout_marginTop="4.0dip"
                        android:layout_weight="1.0"
                        android:orientation="horizontal" >

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="fill_parent"
                            android:layout_gravity="center"
                            android:layout_marginLeft="3.0dip"
                            android:layout_weight="0.3"
                            android:orientation="horizontal" >

                            <TextView
                                android:id="@+id/signup_textView_phone"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:text="Video:"
                                android:textColor="#ffffffff"
                                android:textSize="15.0sp" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="fill_parent"
                            android:layout_marginRight="3.0dip"
                            android:background="@drawable/background_textbox"
                            android:orientation="horizontal" >

                            <TextView
                                android:id="@+id/videoText"
                                android:layout_width="160.0dip"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="12.0dip"
                                android:layout_marginTop="15.0dip"
                                android:paddingLeft="15.0dip"
                                android:paddingRight="15.0dip"
                                android:singleLine="true"
                                android:text=""
                                android:textColor="#ffffffff"
                                android:textSize="15.0sp" />

                            <ImageView
                                android:id="@+id/video"
                                android:layout_width="50.0dip"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="8.0dip"
                                android:src="@drawable/video" />
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="4.0dip"
                        android:layout_marginTop="4.0dip"
                        android:layout_weight="1.0"
                        android:orientation="horizontal" >

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="fill_parent"
                            android:layout_gravity="center"
                            android:layout_marginLeft="3.0dip"
                            android:layout_weight="0.3"
                            android:orientation="horizontal" >

                            <TextView
                                android:id="@+id/signup_textView_phone"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:text="Picture:"
                                android:textColor="#ffffffff"
                                android:textSize="15.0sp" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="fill_parent"
                            android:layout_marginRight="3.0dip"
                            android:background="@drawable/background_textbox"
                            android:orientation="horizontal" >

                            <TextView
                                android:id="@+id/pictureText"
                                android:layout_width="160.0dip"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="12.0dip"
                                android:layout_marginTop="15.0dip"
                                android:paddingLeft="15.0dip"
                                android:paddingRight="15.0dip"
                                android:singleLine="true"
                                android:text=""
                                android:textColor="#ffffffff"
                                android:textSize="15.0sp" />

                            <ImageView
                                android:id="@+id/picture"
                                android:layout_width="50.0dip"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="8.0dip"
                                android:src="@drawable/camera" />
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="4.0dip"
                        android:layout_marginTop="4.0dip"
                        android:layout_weight="1.0"
                        android:orientation="horizontal" >

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="fill_parent"
                            android:layout_gravity="center"
                            android:layout_marginLeft="3.0dip"
                            android:layout_weight="0.3"
                            android:orientation="horizontal" >

                            <TextView
                                android:id="@+id/signup_textView_phone"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:text="Barcode:"
                                android:textColor="#ffffffff"
                                android:textSize="15.0sp" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="fill_parent"
                            android:layout_marginRight="3.0dip"
                            android:background="@drawable/background_textbox"
                            android:orientation="horizontal" >

                            <TextView
                                android:id="@+id/barcodeText"
                                android:layout_width="160.0dip"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="12.0dip"
                                android:layout_marginTop="15.0dip"
                                android:paddingLeft="15.0dip"
                                android:paddingRight="15.0dip"
                                android:singleLine="true"
                                android:text=""
                                android:textColor="#ffffffff"
                                android:textSize="15.0sp" />

                            <ImageView
                                android:id="@+id/barcode"
                                android:layout_width="50.0dip"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="8.0dip"
                                android:src="@drawable/bar_code" />
                        </LinearLayout>
                    </LinearLayout>

                    <RelativeLayout
                        android:layout_width="fill_parent"
                        android:layout_height="40.0dip"
                        android:layout_marginTop="4.0dip"
                        android:orientation="horizontal" >

                        <Button
                            android:id="@+id/save"
                            android:layout_width="100.0dip"
                            android:layout_height="wrap_content"
                            android:layout_alignParentBottom="true"
                            android:layout_centerInParent="true"
                            android:layout_margin="4.0dip"
                            android:background="@drawable/backgrund_button_red"
                            android:text="save" />

                        <Button
                            android:id="@+id/cancel"
                            android:layout_width="100.0dip"
                            android:layout_height="wrap_content"
                            android:layout_margin="4.0dip"
                            android:layout_toRightOf="@id/save"
                            android:background="@drawable/backgrund_button_red"
                            android:text="cancel" />
                    </RelativeLayout>
                </LinearLayout>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>

</LinearLayout>